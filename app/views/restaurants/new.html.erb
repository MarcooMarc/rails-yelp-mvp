<h1>Insert restaurant</h1>

<%= simple_form_for (@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category %>
  <%= f.input :reviews %>
  <%= f.submit "add a restaurant", class: "btn btn-primary" %>


<%=link_to "restaurant", restaurants_path%>


<% end %>
